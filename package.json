{
  "name": "bedybot_api",
  "version": "2022.8.1",
  "private": true,
  "description": "BedyBot - API. Api for get summoner info, summoner rank and another information.",
  "main": "./src/bin/www",
  "scripts": {
    "start": "node ./src/bin/www",
    "start:nodemon": "nodemon ./src/bin/www",
    "eslint": "eslint .",
    "eslintFix": "eslint . --fix",
    "npm:cleanState": "npm ci",
    "npm:outdated": "npm outdated",
    "npm:refreshLock": "npm i --package-lock-only",
    "test": "mocha ./tests/*.spec.js --timeout 5000",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:create": "npx sequelize-cli migration:generate --name="
  },
  "dependencies": {
    "axios": "^0.27.2",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "debug": "~4.3.4",
    "express": "^4.18.2",
    "fs": "0.0.1-security",
    "helmet": "^6.0.0",
    "http-errors": "^2.0.0",
    "https": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "morgan": "~1.10.0",
    "node-cache": "^5.1.2",
    "nodemailer": "^6.8.0",
    "qs": "^6.11.0",
    "sequelize": "^6.25.0",
    "sqlite3": "^5.1.2",
    "util": "^0.12.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.25.0",
    "mocha": "^10.0.0"
  }
}
